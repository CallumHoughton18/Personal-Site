{
  "name": "callums-blog",
  "description": "Personal site, built using Gatsby, TypeScript and Tailwind",
  "version": "1.0.0",
  "private": true,
  "author": "Callum Houghton <callum.houghton13@hotmail.co.uk",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "gen_graphql_types": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "autoprefixer": "^10.3.1",
    "gatsby": "^3.0.0",
    "gatsby-plugin-feed": "3.0.0-v3rc.0",
    "gatsby-plugin-image": "^1.10.1",
    "gatsby-plugin-sass": "^4.10.0",
    "gatsby-plugin-sharp": "^3.10.2",
    "gatsby-remark-prismjs": "5.11.0-next.2",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-remark": "^4.7.0",
    "gatsby-transformer-sharp": "^3.10.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.3.6",
    "prismjs": "^1.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.36.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tailwindcss-filters": "^3.0.0",
    "tsconfig-paths-webpack-plugin": "3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-apollo": "2.3.1",
    "@types/node": "14.14.6",
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "babel-plugin-root-import": "6.6.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.24",
    "prettier": "2.3.2",
    "tailwindcss-textshadow": "^2.1.3",
    "typescript": "4.3.5"
  }
}
